{
	"name": "PayTracker Transaction Extractor",
	"description": "Extract transaction data from PayTracker SaaS platform",
	"version": "3.0.1",
	"manifest_version": 3,
	"icons": {
		"16": "favicon-16x16.png",
		"32": "favicon-32x32.png",
		"48": "icon-48x48.png",
		"128": "icon-128x128.png"
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "favicon-16x16.png",
			"32": "favicon-32x32.png",
			"48": "icon-48x48.png",
			"128": "icon-128x128.png"
		},
		"default_title": "PayTracker Transaction Extractor"
	},
	"permissions": [
		"activeTab",
		"scripting",
		"storage",
		"windows"
	],
	"host_permissions": [
		"*://*/*"
	]
}
